{
	"name": "@melodev/solid-datepicker",
	"version": "0.1.1",
	"description": "extensible datepicker component for solid applications",
	"main": "./lib/cjs/index.js",
	"module": "./lib/esm/index.js",
	"types": "./lib/esm/index.d.ts",
	"files": [
		"lib/"
	],
	"scripts": {
		"clean": "rimraf lib",
		"copy-files": "copyfiles -e **/*.ts -e **/*.tsx -u 1 lib/esm",
		"tsc": "npm run clean && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && npm run copy-files",
		"prepare": "npm run tsc",
		"prepublishOnly": "npm test && npm run prettier && npm run lint",
		"build": "npm build:esm && npm build:cjs",
		"build:esm": "tsc",
		"build:cjs": "tsc --module commonjs --outDir lib/cjs"
	},
	"private": "false",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mrchernicharo/solid-datepicker.git"
	},
	"devDependencies": {
		"typescript": "^4.7.3"
	},
	"dependencies": {
		"copyfiles": "^2.4.1",
		"rimraf": "^3.0.2",
		"solid-js": "^1.4.7",
		"solid-styled-components": "^0.28.4",
		"solid-transition-group": "^0.0.10"
	},
	"keywords": [
		"datepicker",
		"date",
		"calendar",
		"solidjs",
		"solid-datepicker"
	],
	"author": "melodev",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mrchernicharo/solid-datepicker/issues"
	},
	"homepage": "https://github.com/mrchernicharo/solid-datepicker#readme"
}
